            </nav>
        </div>
    </header>
    <!--/header-->
    <section class="w3l-about-breadcrumb">
        <div class="breadcrumb-bg breadcrumb-bg-about py-5">
            <div class="container py-lg-4 py-md-3">
                <h2 class="title">İletişim</h2>
                <ul class="breadcrumbs-custom-path mt-2 text-center">
                    <li><a href="@Url.Action("Index","Anasayfa")">Anasayfa</a></li>
                    <li class="mx-2">/ </li>
                    <li><a href="@Url.Action("Index","Hakkimizda")">Hakkımızda</a></li>
                    <li class="mx-2">/ </li>
                    <li class="active"> İletişim </li>
                </ul>
            </div>
        </div>
    </section>

        <section class="w3l-contact-7 py-5">
        <div class="contacts-9 py-lg-5 py-md-4 py-2">
            <div class="container">
                <h3 class="title-big mb-4"> <span>İletişime Geçelim</span></h3>
                <div class="row map-content-9">
                     
                    <partial name="YorumYap" />
                    <div class="col-lg-6 map mt-lg-0 mt-5">
                        <iframe src=@ViewBag.adressource"
                                frameborder="0" style="border:0" allowfullscreen=""></iframe>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <section class="w3l-content py-5" id="whoweare" style="margin-top:-5em;">
        <div class="content-4-main py-lg-5 py-md-4">
            <div class="container">
                <div class="content-info-in row">
                    <div class="content-left col-lg-4">
                        <h1 class="text-center">TELEFON</h1>
                        <p class="" style="text-align:center;">

                        <b>  @ViewBag.telefon</b>    
                        </p>
                    </div>
                    <div class="content-left col-lg-4">
                        <h1 class="text-center">FAX</h1>
                        <p class="" style="text-align:center;">
                            <b>  @ViewBag.fax</b>
                        </p>
                    </div>
                    <div class="content-left col-lg-4">
                            <h1 class="text-center">EMAIL</h1>
