@model BeeBilisim.Web.ViewModels.IletisimViewModel

@{
    Layout = null;

    var deger = Model.IletisimModels.Where(x => x.Id == 1);
    ViewBag.adresmetin = deger.Select(x => x.AdresMetin).FirstOrDefault();
    ViewBag.adressource = deger.Select(x => x.AdresIFrame).FirstOrDefault();
    ViewBag.email = deger.Select(x => x.Email).FirstOrDefault();
    ViewBag.fax = deger.Select(x => x.Fax).FirstOrDefault();
    ViewBag.telefon = deger.Select(x => x.Telefon).FirstOrDefault();

}

<!doctype html>
<html lang="en">
<head>
