@using BeeBilisim.Web.Models
@model List<HakkimizdaModel>
@{
    Layout = "";

}

<br>
<h2 class="text-center">BİZ KİMİZ KISMI BİLGİSİ</h2>
<br />
<table class="table table-bordered col-md-12">
    <thead >
        <tr >
            <th scope="col">Metin 1 (Sol)</th>
            <th scope="col">Metin 2 (Sağ)</th>
            <th scope="col" style="text-align:center;">Güncelle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.BizKimizMetin1</td>
                <td>@x.BizKimizMetin2</td>
                <td style="text-align:center;"><a href="/AdminPanel/BizKimizGetir/@x.Id" class="btn btn-warning">Güncelle</a></td>
            </tr>
        }
    </tbody>
</table>

