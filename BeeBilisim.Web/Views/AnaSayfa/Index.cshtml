    ViewBag.slogan = deger.Select(x => x.Slogan).FirstOrDefault();
    ViewBag.sloganAciklama = deger.Select(x => x.SloganAciklama).FirstOrDefault();
    ViewBag.telefon = deger.Select(x => x.Telefon).FirstOrDefault();
    ViewBag.fax = deger.Select(x => x.Fax).FirstOrDefault();
    ViewBag.mail = deger.Select(x => x.Email).FirstOrDefault();
    ViewBag.govdemetinbasligi = deger.Select(x => x.GovdeMetinBasligi).FirstOrDefault();
    ViewBag.govdemetinaciklama1 = deger.Select(x => x.GovdeMetinAciklama1).FirstOrDefault();
    ViewBag.govdemetinaciklama2 = deger.Select(x => x.GovdeMetinAciklama2).FirstOrDefault();
    ViewBag.nedenbizmetin = deger.Select(x => x.NedenBizMetin).FirstOrDefault();
    ViewBag.nedenbizsol1 = deger.Select(x => x.NedenBizSol1).FirstOrDefault();
    ViewBag.nedenbizsol2 = deger.Select(x => x.NedenBizSol2).FirstOrDefault();
    ViewBag.nedenbizsol3 = deger.Select(x => x.NedenBizSol3).FirstOrDefault();
    ViewBag.nedenbizsag1 = deger.Select(x => x.NedenBizSag1).FirstOrDefault();
    ViewBag.nedenbizsag2 = deger.Select(x => x.NedenBizSag2).FirstOrDefault();
    ViewBag.nedenbizsag3 = deger.Select(x => x.NedenBizSag3).FirstOrDefault();
